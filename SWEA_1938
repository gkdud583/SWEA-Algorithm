
import java.util.*;
import java.io.*;



public class SWEA_1938 {
    public static void main(String[]args) throws Exception{

        Scanner sc = new Scanner(System.in);
        
        int a = sc.nextInt();
        int b = sc.nextInt();

        System.out.println(a + b);
        System.out.println(a - b);
        System.out.println(a * b);
        System.out.println((int)(a / (double)b));

    }
}