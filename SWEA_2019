
import java.util.*;
import java.io.*;



public class SWEA_2019 {
    public static void main(String[]args) throws Exception{

        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();
        int v = 1;
        System.out.print(1+" ");
        for(int i=1; i<=n; i++){
            v *= 2;
            System.out.print(v +" ");
        
        }

        
    }
}